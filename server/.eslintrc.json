{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/no-import-module-exports": "off",
        "no-underscore-dangle": "off",
        "no-use-before-define": "off",
        "import/prefer-default-export": "off"
    }
}